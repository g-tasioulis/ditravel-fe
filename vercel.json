{
  "version": 2,
  "builds": [
    { "src": "src/app/next.config.js", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/about", "dest": "/src/app/about/page.tsx" },
    { "src": "/contact", "dest": "/src/app/contact/page.tsx" },
    { "src": "/other-page", "dest": "/src/app/other-page/page.tsx" },
    { "src": "/(.*)", "dest": "/src/app/$1" }
  ]
}
